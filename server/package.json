{
  "name": "cloudsphere-server",
  "version": "1.0.0",
  "description": "Enterprise-grade backend server for CloudSphere Dashboard",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "nodemon src/index.js",
    "build": "babel src -d dist",
    "build:watch": "babel src -d dist --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "keywords": [
    "express",
    "mongodb",
    "api",
    "backend",
    "enterprise"
  ],
  "author": "CloudSphere Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.1",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
